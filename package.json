  "author": "Enzo Innocenzi",
  "license": "MIT",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs",
      "types": "./dist/index.d.ts"
    }
  },
  "main": "dist/index.cjs",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "files": [
    "dist",
    "*.d.ts"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/innocenzi/vite-plugin-mjml"
  },
  "homepage": "https://github.com/innocenzi/vite-plugin-mjml#readme",
  "bugs": "https://github.com/innocenzi/vite-plugin-mjml/issues",
  "scripts": {
    "prepare": "npm run build",
    "build": "unbuild",
    "test": "vitest",
    "lint": "eslint src/*.ts",
    "release": "bumpp --push --tag --commit \"release: v\""
  },
  "dependencies": {
    "debug": "^4.3.4",
    "fast-glob": "^3.3.2",
    "mjml": "^4.14.1",
    "mjml-core": "^4.14.1",
    "picocolors": "^1.0.0"
  },
  "devDependencies": {
    "@innocenzi/eslint-config": "^0.21.2",
    "@types/debug": "^4.1.12",
    "@types/mjml": "^4.7.4",
    "@types/mjml-core": "^4.7.4",
    "@types/node": "^18.19.9",
    "bumpp": "^9.3.0",
    "eslint": "^8.56.0",
    "typescript": "^5.3.3",
    "unbuild": "^2.0.0",
    "vite": "^3.2.8",
    "vitest": "^1.2.1"
